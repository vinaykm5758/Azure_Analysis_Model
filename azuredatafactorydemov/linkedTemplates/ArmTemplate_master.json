{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredatafactorydemov"
		},
		"LS_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage'"
		},
		"LS_Azure_SQL_Server_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Azure_SQL_Server'"
		},
		"LS_SqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SqlServer'",
			"defaultValue": "Integrated Security=True;Data Source=LAPTOP-5VARS294;Initial Catalog=AdventureWorksLT2016"
		},
		"LS_SqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SqlServer'"
		},
		"LS_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurestoragedemov.dfs.core.windows.net/"
		},
		"LS_SqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vinay"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredatafactorydemov_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage_accountKey')]"
					},
					"LS_Azure_SQL_Server_connectionString": {
						"value": "[parameters('LS_Azure_SQL_Server_connectionString')]"
					},
					"LS_SqlServer_connectionString": {
						"value": "[parameters('LS_SqlServer_connectionString')]"
					},
					"LS_SqlServer_password": {
						"value": "[parameters('LS_SqlServer_password')]"
					},
					"LS_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"LS_SqlServer_properties_typeProperties_userName": {
						"value": "[parameters('LS_SqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}